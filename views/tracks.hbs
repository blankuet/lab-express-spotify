<div class="flex flex-col">
    {{#each tracks}}
    <div class="flex justify-between items-center even:bg-gray-200 odd:bg-white-p-2">
        <p>{{this.name}}</p>
        {{#each this.artists}}
        <p>{{this.name}}</p>
        {{/each}}
        {{#if this.preview_url}}
        <audio controls>
            <source src="{{this.preview_url}}"/>
        </audio>
        {{else}}
        <p>No preview available</p>
        {{/if}}
    </div>
    {{/each}}
</div>